on init
    insmod /system/lib/modules/ecryptfs.ko
    insmod /system/lib/modules/texfat.ko

    wait /dev/block/platform/msm_sdcc.1
    symlink /dev/block/platform/msm_sdcc.1 /dev/block/bootdevice

on boot
    start choice_fn
    start chargeled

on offmode_charging
    start offmode_charging

on property:ro.boot.mode=offmode_charging
    stop adbd
    stop recovery

service chargeled /sbin/chargeled
    disabled
    seclabel u:r:healthd:s0

service choice_fn /sbin/choice_fn
    oneshot
    seclabel u:r:recovery:s0

service detect_key /sbin/detect_key
    disabled
    seclabel u:r:healthd:s0
    oneshot

service offmode_charging /sbin/offmode_charging
    disabled
    seclabel u:r:healthd:s0

service power_test /sbin/power_test
    disabled
    oneshot
